#define _GNU_SOURCE
#include <stdlib.h>
#include <string.h>
#include <unistd.h>

int	main(int argc,char **argv)
{
	int		check;
	char	*str;
	int		uid;
	int		group;
  
	check = atoi(argv[1]);
	if (check == 423)
	{
		str = strdup("/bin/sh");
		group = getegid();
		uid = geteuid();
		setresgid(group, group, group);
		setresuid(uid, uid, uid);
		execv("/bin/sh", &str);
	}
	else
		write(2, "No !\n", 5);

	return (0);
}